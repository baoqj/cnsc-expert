# Frontend (optional override)
VITE_API_BASE_URL=/api

# Prisma / Neon Postgres
DATABASE_URL="postgresql://<user>:<password>@<host>/<database>?sslmode=require"

# Vercel serverless function (required in Vercel Project Environment Variables)
DIFY_BASE_URL=https://your-dify-domain-or-ip
DIFY_APP_API_KEY=app-xxxxxxxxxxxxxxxx
DIFY_TIMEOUT_MS=60000

# Optional seed variables
SEED_ADMIN_EMAIL=admin@cnsc.local
SEED_ADMIN_NAME=CNSC Admin
SEED_ORG_NAME=CNSC Default Organization
SEED_ORG_SLUG=cnsc-default-organization
